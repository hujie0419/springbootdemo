
@{
    ViewBag.Title = "单播模版编辑";
    var batchid = ViewBag.batchid as string;
}
@section head{
    <link href="~/Content/nfine/js/bootstrap/bootstrap.min.css" rel="stylesheet" />
}
<style>
    .tdtitle {
        background-color: #e8eef4;
        text-align: center;
    }
</style>

@Html.Hidden("batchid", batchid)

<div class="content" id="EditSinglePushTemplate">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <table style="width: 100%;table-layout:fixed">
                    <tr>
                        <th colspan="3" class="tdtitle">沟通计划</th>
                    </tr>
                    <tr>
                        <td>计划id</td>
                        <td colspan="2">123</td>
                    </tr>
                    <tr>
                        <td>计划状态</td>
                        <td colspan="2">123</td>
                    </tr>
                    <tr>
                        <td>计划名称</td>
                        <td colspan="2">
                            <textarea style="width:100%"></textarea>
                        </td>
                    </tr>
                    <template v-for="template in Templates">
                        <tr>
                            <td colspan="3" class="tdtitle">
                                {{template.PushTypeDesc}}
                            </td>
                        </tr>
                        <tr>
                            <td>操作</td>
                            <td>
                                <button>编辑</button>
                            </td>
                        </tr>
                        <tr>
                            <td>是否启用</td>
                            <td colspan="2">
                                <select v-model="template.IsEnable">
                                    <option value="1">启用</option>
                                    <option value="0">禁用</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>标题</td>
                            <td colspan="2">
                                {{template.TemplateTitle}}
                            </td>
                        </tr>
                    </template>
                    <tr>
                        <td>
                            <button class="btn">关闭</button>
                        </td>
                        <td>
                            <button class="btn">保存草稿</button>
                        </td>
                        <td>
                            <button class="btn">提交发送</button>
                        </td>

                    </tr>
                </table>

            </div>
            <div class="col-md-6">
                <table style="width: 100%; table-layout: fixed">
                    <tr>
                        <th class="tdtitle" colspan="4">内容设置</th>
                    </tr>
                    <tr>
                        <td>推送标题</td>
                        <td colspan="3">
                            <textarea style="width:100%" v-model="iostemplate.Title"></textarea>
                        </td>
                    </tr>
                    <tr>
                        <td>推送副标题</td>
                        <td colspan="3">
                            <textarea style="width:100%" v-model="iostemplate.SubTitle"></textarea>
                        </td>
                    </tr>
                    <tr>
                        <td>推送内容</td>
                        <td colspan="3">
                            <textarea style="width:100%" v-model="iostemplate.Content"></textarea>
                        </td>
                    </tr>
                    <tr>
                        <td>外部参数说明</td>
                        <td>
                            <button>添加</button>
                        </td>
                    </tr>
                    @*<tr>
                        <td></td>
                        <td>参数1</td>
                        <td colspan="2">
                            <input type="text" />
                        </td>
                    </tr>*@
                    <tr v-for="desc in iosdesc">
                        <td>
                            <input type="text" v-model="desc.Key" />
                        </td>
                        <td colspan="2">
                            <input type="text" v-model="desc.Value" />
                        </td>
                    </tr>
                    <tr>
                        <td>推送图片</td>
                        <td>
                            <img style="width:80px" src="{{iostemplate.ImageUrl}}" />
                        </td>
                        <td>
                            <input type="file" />
                        </td>
                    </tr>
                    <tr>
                        <td class="tdtitle" colspan="4">APP内操作</td>
                    </tr>
                    <tr>
                        <td>键值对参数</td>
                        <td>
                            <button>添加</button>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="3">
                            <input type="text" style="width:100%" />
                        </td>
                        <td>
                            <button>删除</button>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="4">
                            <textarea style="width:100%"></textarea>
                        </td>
                    </tr>
                    <tr>
                        <td class="tdtitle" colspan="4">
                            高级选项
                        </td>
                    </tr>
                    <tr>
                        <td>消息过期周期</td>
