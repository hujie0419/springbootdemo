        },
        RemoveSearchAllCookie:function(isReset){
            var delcookies = ["data-page","data-rim","data-tab","data-brand"];
            for (var i = 0; i < delcookies.length; i++) {
                CookieTools.DelCookie(delcookies[i]);
            }

            if((isReset || false)){
                this.RestForm(1);
            }
        },
        RestForm:function(i){
            i = i || 0;
            if(i==1){
                $("#ParamsListForm input[data-brand]").attr("checked",false);
                $("#ParamsListForm input[data-tab]").attr("checked",false);
                $("#ParamsListForm input[data-rim]").attr("checked",false);
            }
            else if(i==2){
                $(".select_s").val("请选择");
                $("#ProductList input[data-bind='SecondSearch']").val("");
                CookieTools.DelCookie("SecondSearchProuds");
            }

        },
        GetSecondSearchProudsModel: function (S_PiceOrderBy) {
            S_PiceOrderBy = S_PiceOrderBy || "";
            var S_Price_Begin = "";//销售价
            var S_Price_End = "";//销售价
            var S_CostPrice_Begin = "";//成本价
            var S_CostPrice_End = "";//成本价
            var S_PriceAfter_Begin = "";//券后销售价
            var S_PriceAfter_End = "";//毛利率
            var S_MLAfter_Begin = "";//券后毛利率
            var S_MLAfter_End = "";//券后毛利率

            var filtrateType = $("#filtrateType option:selected").val();
            var S_PID = $.trim($("#ProductList input[data-id='S_PID']").val());
            var S_Coupon = $.trim($("#ProductList input[data-id='S_Coupon']").val());
            var S_Figure = $("#S_Figure option:selected").text()=="请选择"?"":$("#S_Figure option:selected").text();
            var pageSize=$("#pageSize option:selected").text()=="请选择"?"":$("#pageSize option:selected").text();
            var onsale=$("#onsale option:selected").text()=="请选择"?"":$("#onsale option:selected").text();
            //var maolisort=$("#maoli option:selected").text()=="请选择"?"":$("#maoli option:selected").val();
            var maoliSort = $("#maoli option:selected").text() == "请选择" ? "" : $("#maoli option:selected").val();
            var S_IsShow = $("#S_IsShow option:selected").text() == "全部" ? "" : $("#S_IsShow option:selected").val();
            var S_Params = "";
            if (S_IsShow) {
                S_Params +="&S_IsShow=" + S_IsShow;
            }
            if (maoliSort)
                S_Params += "&maoliSort=" + maoliSort;
            //if(S_Price_Begin.length > 0 && S_Price_End.length > 0 && isNaN(S_Price_Begin) == false && isNaN(S_Price_End) == false)
            //    S_Params +="&S_Price=" + S_Price_Begin + "|"+S_Price_End;
            if(S_PID.length > 0)
                S_Params +="&S_PID=" + S_PID;
            if(S_Coupon.length > 0)
                S_Params +="&S_Coupon=" + S_Coupon;
            if(S_Figure.length > 0)
                S_Params +="&S_Figure=" + S_Figure;

            if (pageSize) {
                S_Params+="&pageSize="+pageSize;
            }
            if (onsale) {
                S_Params+="&onsale="+onsale;
            }
            if (filtrateType) {
                S_Params += "&filtrateType=" + filtrateType;
            }
            if (filtrateType == "SalePrice") {
                S_Price_Begin = $.trim($("#ProductList input[data-id='S_Price_Begin']").val());
                S_Price_End = $.trim($("#ProductList input[data-id='S_Price_End']").val());
                if (S_Price_Begin && S_Price_End) {
                    S_Params += "&S_Price=" + S_Price_Begin + "|" + S_Price_End;
                }
            }
            if (filtrateType == "CostPrice") {
                S_CostPrice_Begin = $.trim($("#ProductList input[data-id='S_Price_Begin']").val());
                S_CostPrice_End = $.trim($("#ProductList input[data-id='S_Price_End']").val());
                if (S_CostPrice_Begin && S_CostPrice_End) {
                    S_Params += "&S_CostPrice=" + S_CostPrice_Begin + "|" + S_CostPrice_End;
                }
            }
            if (filtrateType == "SalePriceAfter") {
                S_PriceAfter_Begin = $.trim($("#ProductList input[data-id='S_Price_Begin']").val());
                S_PriceAfter_End = $.trim($("#ProductList input[data-id='S_Price_End']").val());
                if (S_PriceAfter_Begin && S_PriceAfter_End) {
                    S_Params += "&SalePriceAfter=" + S_PriceAfter_Begin + "|" + S_PriceAfter_End;
                }
            }
            if (filtrateType == "GrossMarginAfter") {
                S_MLAfter_Begin = $.trim($("#ProductList input[data-id='S_Price_Begin']").val());
                S_MLAfter_End = $.trim($("#ProductList input[data-id='S_Price_End']").val());
                if (S_MLAfter_Begin && S_MLAfter_End) {
                    S_Params += "&maoliAfter=" + S_MLAfter_Begin + "|" + S_MLAfter_End;
                }
            }
            //if (maolisort) {
            //    S_Params+="&maolisort="+maolisort;
            //}
            //if (S_ML_Begin&&S_ML_End) {
            //    S_Params+="&maoli="+S_ML_Begin+"|"+S_ML_End;
            //}
            var model = new this.SecondSearchProudsModel();
            model.S_Price_Begin = $.trim($("#ProductList input[data-id='S_Price_Begin']").val());
            model.S_Price_End = $.trim($("#ProductList input[data-id='S_Price_End']").val());
            model.S_PID = S_PID;
            model.S_Coupon = S_Coupon;
            model.S_Figure = S_Figure;
            model.S_Params = S_Params;
            model.S_ML_Begin="";
            model.S_ML_End="";
            model.maoliSort = maoliSort;
            model.pageSize=pageSize;
            model.onsale=onsale;
            model.S_price = "";
            model.S_ConstPrice = "";
            model.S_IsShow = S_IsShow;
            model.FiltrateType = filtrateType;
            return model;
        },
        SecondSearchProuds:function(S_PiceOrderBy,isOrderByBut)
        {
            isOrderByBut = isOrderByBut || false;
            var model = this.GetSecondSearchProudsModel(S_PiceOrderBy);
            if(model != null){
                CookieTools.SetCookie("SecondSearchProuds",JSON.stringify(model));
                if(isOrderByBut){
                    this.SearchProuds(CookieTools.GetCookie("data-page"),true,model.S_Params);
                }
                else{
                    this.SearchProuds(1,true,model.S_Params);
                    CookieTools.SetCookie("data-page",1);
                }
            }
            CookieTools.SetCookie("SearchType",2);
        },
