@{
    ViewBag.Title = "通过UserId批量登出用户";
}
<script src="~/Scripts/ajaxfileupload.js"></script>
<h2>通过UserId批量登出用户</h2>

<form method="post" action="" enctype="multipart/form-data">
    <input type="file" id="btnImport" class="btnImport" name="btnImport">
</form>
<script type="text/javascript">
    $('#btnImport').on('change', function () {
        var id = $(this).attr('id');
        $.ajaxFileUpload({
            url: '/UserManager/BatchLogOutUserByUserIdAction',
            secureuri: false,
            fileElementId: id,
            dataType: 'json',
            async: false,
            data: {},
            type: "post",
            success: function (result) {
                console.log(result);
                result = JSON.parse(result);
                if (result.Status == 0) {
                    alert("操作成功！");
                } else {
                    alert(html(result.Error));
                }
                alert(result);
            },
            error: function (data) //服务器响应失败处理函数
            {
                alert(JSON.stringify(data));
            }
        });
    });
</script>

