<style type="text/css">
    .toast-middle-center{
        position:fixed;
        bottom:20%;
        left:10%;
        right:10%;
        font:20px arial;
        line-height:18px;
    }
    .toast-middle-center div.toast.toast-info{
        width:100% !important;
    }
    .toast-middle-center div.toast.toast-info div.toast-message{
        line-height:24px;
    }
    .hidden {
        display: none !important;
    }
    .adv.bottom{
        position:fixed !important;
        width:100% !important;
        bottom:0 !important;
    }
    a.bdisabled{
        background-color:#c5c3c3;
        color:gray;
    }
    a.btn:active,a.btn:visited{color:#fff;}
</style>
<script type="text/javascript">
    toastr.options.positionClass = 'toast-middle-center';
    toastr.options.timeOut = 1000;
</script>
