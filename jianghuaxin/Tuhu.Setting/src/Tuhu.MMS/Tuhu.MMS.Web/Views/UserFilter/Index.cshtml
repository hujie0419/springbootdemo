@model IEnumerable<Tuhu.MMS.Web.Domain.UserFilter.UserFilterRuleJob>
@{
    ViewBag.Title = "Index";
}


<div class="ibox-content">
    <h1 class="font-bold">
        会员筛选Job配置列表
    </h1>

    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Job描述</th>
                    <th>状态</th>
                    <th colspan="4">操作</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    @if (Model != null && Model.Any())
                    {
                        foreach (var userFilterRuleJob in Model)
                        {
                        <tr>
                            <td>@userFilterRuleJob.PKID</td>
                            <td>@userFilterRuleJob.Description</td>
                            <td>@userFilterRuleJob.JobStatus</td>
                            <td>
                                <a href="@Url.Action("EditUserFilterRule",new { jobid =userFilterRuleJob.PKID})">筛选条件</a>
                            </td>
                            <td>
                                <a href="@Url.Action("SelectFilterRuleOutResult",new { jobid =userFilterRuleJob.PKID})">应用字段</a>
                            </td>
                            <td>删除</td>
                            <td>操作历史</td>
                        </tr>
                        }
                    }
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
