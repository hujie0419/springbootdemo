                @*@if ( Model.MenuType == 1)
                    {
                        <input type="radio" value="1" name="rbMenuType" checked="checked" /><span>滑动菜单</span>
                        <input type="radio" value="0" name="rbMenuType" /><span>滚动菜单</span>
                    }
                    else if(Model.MenuType == 0)
                    {
                        <input type="radio" value="1" name="rbMenuType" /><span>滑动菜单</span>
                        <input type="radio" value="0" name="rbMenuType" checked="checked" /><span>滚动菜单</span>
                    }*@
