                }
                var pkid = columns.eq(0).attr("id");
                jsonData.PKID = pkid != null && pkid != "" ? pkid : 0;
                jsonData.Grade = grade;
                jsonData.IsShopInstall = IsShopInstall;
                jsonData.Content = content;
                jsonData.Price = price;
                jsonData.ContainInstall = containInstall;
                jsonData.IsFreeInstall = isFreeInstall1;
                jsonData.ProductType = 1;

                arrRank.push(jsonData);
