@using Tuhu.Provisioning.Models
@model Tuhu.Component.Common.Models.ListModel<Tuhu.Provisioning.DataAccess.Entity.BaoYangPackageBrandPriorityConfig>
@{
    Layout = null;
}
<div>
    <table id="table1" style="width:100%;margin-top:20px;">
        <thead>
            <tr>
                <th><input type="checkbox" onclick="BatchToggleCheckoutBox()"/></th>
                <th style="display:none;">Id</th>
                <th>VehicleID</th>
                <th>品牌</th>
                <th>系列</th>
                <th>车价格</th>
                <th>套餐品牌优先级</th>
                <th>机油等级优先级</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <input type="checkbox" />
                    </td>
                    <td style="display:none;">@item.Id</td>
                    <td>@item.VehicleID</td>
                    <td>@item.Brand</td>
                    <td>@item.Vehicle</td>
                    <td>@item.MinPrice</td>
                    <td>@item.PackageBrands</td>
                    <td>@item.JiYouGrade</td>
                    <td>
                        <a href="javascript:;" onclick="BaoYangPackageBrandPriorityConfig.Update(@item.Id,'edit','@item.VehicleID')">编辑</a>
                        <a href="javascript:;" onclick="BaoYangPackageBrandPriorityConfig.Delete(@item.Id)">删除</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

<div>
    @Html.Pager(Model.Pager, pageIndex => Url.RouteUrl(new { pageIndex }))
</div>
<div id="AddItem"></div>
