                var isFreeInstall1 = false;
                if (IsShopInstall) {
                    var pro = columns.eq(2).find("input[type='text'").attr("isfreeinstall");
                    if (pro === "true") {
                        isFreeInstall1 = true;
                    }
                }
                var grade = null;
                var content = columns.eq(0).find("input[type='text'").val();
                var price = 0;
                var containInstall = columns.eq(2).find("input[type='text'").val();
                if (content === null || content === "") {
                    at += "内容未填写\n";
                }
                if (containInstall === null || containInstall === "") {
