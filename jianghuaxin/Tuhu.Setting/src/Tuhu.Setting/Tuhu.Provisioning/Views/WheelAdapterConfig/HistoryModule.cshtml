@using Tuhu.Provisioning.DataAccess.Entity;
@using Tuhu.Provisioning.Controllers;
@model List<WheelAdapterConfigLog>
@{
    Layout = null;
    ViewBag.Title = "HistoryModule";
}

<table style="background: #ffffff; font-size: 12px; width: 100%; text-align:center" border="1" class="table">
    <tr>
        <th style="text-align: center">操作人</th>
        <th style="text-align: center">时间</th>
        <th style="text-align: center">操作类型</th>
    </tr>
    @if (Model.Any())
    {
        foreach (var share in Model)
        {
            <tr>
                <td name="Operator">@share.Operator</td>
                <td name="LastUpdateDateTime">@share.LastUpdateDateTime</td>
                <td name="OperateType">@(share.OperateType == 0 ? "创建" : "修改")</td>
            </tr>
        }
    }
</table>
