<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
    <processing-directives>
      <overwrite-existing-data>true</overwrite-existing-data>
    </processing-directives>
    <pre-processing-commands></pre-processing-commands>
    <schedule>
      <job>
        <name>ActivityJob</name>
        <group>DefaultGroup</group>
        <description>自动审核活动JOB</description>
        <job-type>Tuhu.C.Job.Activity.Job.ActivityJob,Tuhu.C.Job.Activity</job-type>
        <durable>true</durable>
        <recover>false</recover>
      </job>
      <trigger>
        <cron>
          <name>ActivityJobTrigger</name>
          <group>DefaultGroup</group>
          <description>系统自动审核上海市闵行区用户的报名状态为已通过</description>
          <job-name>ActivityJob</job-name>
          <job-group>DefaultGroup</job-group>
          <cron-expression>0 * * * * ? *</cron-expression>
        </cron>
      </trigger>
    </schedule>
  </job-scheduling-data>
