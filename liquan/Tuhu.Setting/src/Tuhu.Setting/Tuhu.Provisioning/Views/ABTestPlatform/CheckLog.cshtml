
@{
    ViewBag.Title = "历史修改记录";
    Layout = null;
}
@using Tuhu.Provisioning.DataAccess.Entity.ABTestPlatform;
@model List<ABTestEditLog>

<table style="background: #ffffff; font-size: 12px; width: 100%; text-align:center" border="1" class="table">
    <tr>
        <th style="text-align: center">测试名</th>
        <th style="text-align: center">修改变更</th>
        <th style="text-align: center">修改时间</th>
        <th style="text-align: center">操作人</th>
    </tr>
    @if (Model.Any())
    {
        foreach (var item in Model)
        {
            <tr>
                <td name="TestName">@item.TestName</td>
                <td name="Change">@item.Change</td>
                <td name="LastUpdateDataTime">@item.LastUpdateDataTime</td>
                <td name="Operator">@item.Operator</td>
            </tr>
        }
    }
</table>

