                    var price = columns.eq(2).find("input[type='text'").val();
                    var containInstall = columns.eq(3).find("input[type='text'").val();
                    if (content === null || content === "") {
                        at += "用户等级" + grade + "内容未填写\n";
                    }
                    if (parseFloat(price) + "" == "NaN" || price === "") {
                        at += "用户等级" + grade + "价格不符合规范\n";
