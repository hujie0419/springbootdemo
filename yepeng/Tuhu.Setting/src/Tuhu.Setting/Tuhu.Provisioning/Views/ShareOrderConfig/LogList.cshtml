
@model IEnumerable<Tuhu.Provisioning.DataAccess.Entity.ConfigHistory>
@{
    Layout = "";
}
<style>
    table th {
        color: white;
        padding: 6px 5px;
        text-align: left;
        background-color: cornflowerblue;
        border: solid 1px cornflowerblue;
    }

    td {
        width: 250px;
    }
</style>
<table>
    <thead>
        <tr>
            <th>操作人</th>
            <th>操作时间</th>
            <th>操作</th>
            <th>数据对比</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Author</td>
                <td>@item.ChangeDatetime</td>
                <td>@item.Operation</td>
                <td>
                    <a style="color:blue" target="_blank" href="/ShareOrderConfig/LogDetail?before=@item.BeforeValue&after=@item.AfterValue">详情</a>
                </td>

            </tr>
        }
    </tbody>
</table>