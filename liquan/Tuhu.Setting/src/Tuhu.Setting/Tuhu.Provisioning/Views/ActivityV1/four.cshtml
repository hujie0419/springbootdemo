<script type="text/javascript">
    function GetRowFour() {
        var list = [];
        var date = (new Date()).Format("yyyyMMddhhmmss");
        var row = {
            'Group': date,
            'PID': $('#threeTableByOnePC [name=PID]').val(),
            'VID': $('#threeTableByOnePC [name=VID]').val(),
            'CID': $('#threeTableByOnePC [name=CID]').val(),
            'SmallImage': $('#threeTableByOnePC [id=cimg]').attr('src')||'', //小图路径
            'Image': $('#threeTableByOnePC [name=hidCimg]').val()||'', //大图
            'BigImg': 4,//一行四列
            'Type': $('#threeTableByOnePC [name=Type]').val(), //类型
            'OrderBy': $('#threeTableByOnePC [name=OrderBy]').val(),
            'LinkUrl': $('#threeTableByOnePC [name=Url]').val(),
            'HandlerIOS': $('#threeTableByOnePC [name=HandlerIOS]').val(),
            'HandlerAndroid': $('#threeTableByOnePC [name=HandlerAndroid]').val(),
            'SOAPIOS': $('#threeTableByOnePC [name=SOAPIOS]').val(),
            'SOAPAndroid': $('#threeTableByOnePC [name=SOAPAndroid]').val(),
            'IsUploading': $('#threeTableByOnePC [name=IsImage]').val()  //$('#threeTableByOne [name=IsUploading]').val() || 0
        };

        row.DisplayWay = $('#threeTableByOnePC [name=DisplayWay]').val();//显示方式

        row.Description = $('#threeTableByOnePC [name=Description]').val();//描述

        if (row.Type == 4) {
            //优惠卷
            row.TwoSImage = $('#threeTableByOnePC [id=cimgTwoPic]').attr('src') || '';
            row.TwoBImage = $('#threeTableByOnePC [name=hidCimgTwoPic]').val();
        }

        //if ($('#threeTableByOne [name=TireBrand]').val() != -1) {
        //    //选择车牌
        //    row.TireBrand = $('#threeTableByOne [name=TireBrand]').val();
        //}

        if ((row.Type == 0 || row.Type == 1 || row.Type == 6 || row.Type == 7) && row.IsUploading == 0) {
            row.ProductName = $('#threeTableByOnePC [name=ProductName]').val();
        }
