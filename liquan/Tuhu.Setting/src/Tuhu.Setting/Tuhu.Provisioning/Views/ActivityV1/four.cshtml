@{
    ViewBag.Title = "four";
    Layout = null;
}



<div style="float:left;margin-left:10px">
    <form id="threeFormOnePC">
        <table id="threeTableByOnePC" class="easyui-form">
            <tr>
                <td colspan="3">
                    <center>一列</center>
                </td>
            </tr>
            <tr>
                <td>PID:</td>
                <td colspan="2">
                    <input type="text" name="PID" onchange="GetProductImage(this, 'threeTableByOnePC')" />
                </td>
            </tr>
            <tr>
                <td>产品名称:</td>
                <td colspan="2">
                    <input type="text" id="ProductName" name="ProductName" />
                </td>
            </tr>
            <tr>
                <td>活动ID：</td>
                <td colspan="2">
                    <input type="text" name="VID"  />
                </td>
            </tr>
            <tr>
                <td>优惠券ID：</td>
                <td colspan="2">
                    <input type="text"  name="CID" onchange="BindCoupon('threeFormOnePC')" />
                    @*<select name="CID" id="CID"></select>*@
                </td>

            </tr>
            <tr>
                <td>图片:</td>
                <td>
                    <img id="cimg" width="20" height="20" />
                    <input type="hidden" id="hidCimg" name="hidCimg" />
                </td>
                <td>
                    <input type="file" id="ThreecFilePC" name="ThreecFilePC" onchange="UploadImg(this, 'cimg', 'hidCimg', 'IsUploading', 'threeTableByOnePC')" />
                    <input type="hidden" id="IsUploading" name="IsUploading" value="@(Model == null?"0":Model.IsUploading.ToString())" />
                </td>
            </tr>
            <tr id="TwoPic" style="display:none;">
                <td>图片(二):</td>
                <td>
                    <img id="cimgTwoPic" width="20" height="20" />
                    <input type="hidden" id="hidCimgTwoPic" name="hidCimgTwoPic" />
                </td>
                <td>
                    <input id="ThreecFileTwoPic1PC" type="file" name="ThreecFileTwoPic1PC" onchange="UploadImg(this, 'cimgTwoPic', 'hidCimgTwoPic', 'IsUploading', 'threeTableByOnePC')" />
                </td>
            </tr>
            <tr>
                <td>
                    活动类型:
                </td>
                <td colspan="2">
                    <select id="Type" name="Type" onchange="DropDownChange('threeFormOnePC')">
                        <option value="-1">请选择</option>
                        <option value="0">轮胎</option>
                        <option value="1">车品</option>
                        <option value="2">链接</option>
                        <option value="3">图片</option>
                        <option value="4">优惠卷</option>
                        <option value="5">保养</option>
                        <option value="6">限购轮胎</option>
                        <option value="7">限购车品</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td>是否图片:</td>
                <td colspan="2">
                    <select id="IsImage" name="IsImage">
                        <option value="0" selected="selected">模板</option>
                        <option value="1">图片</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td>
                    显示方式:
                </td>
                <td colspan="2">
                    <select id="DisplayWay" name="DisplayWay">
                        <option value="-1">不选择</option>
                        <option value="0">竖向</option>
                        <option value="1">横向</option>

                    </select>
                </td>
            </tr>
            <tr>
                <td>描述:</td>
                <td colspan="2">

                    <textarea name="Description" id="Description" style="width:280px;height:16px; font-size:12px; overflow:hidden;"></textarea>
                </td>
            </tr>
            <tr>
                <td>官网URL:</td>
                <td colspan="2">
                    <input type="text" name="Url" />
                </td>
            </tr>

            <tr>
                <td>排序:</td>
                <td colspan="2">
                    <input type="text" name="OrderBy" />
                </td>
            </tr>
            <tr style="display:none;">
                <td>轮胎品牌:</td>
                <td colspan="2">
                    <select id="TireBrand" name="TireBrand">
                        <option value="-1">不选择</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td colspan="2">APP处理值(IOS):</td>
                <td>
                    <input type="text" name="HandlerIOS" value="" />
                </td>
            </tr>
            <tr>
                <td colspan="2">APP处理值(Android):</td>
                <td>
                    <input type="text" name="HandlerAndroid" value="" />
                </td>
            </tr>
            <tr>
                <td colspan="2">特殊通讯值(IOS):</td>
                <td>
                    <input type="text" name="SOAPIOS" />
                </td>
            </tr>
            <tr>
                <td colspan="2">特殊通讯值(Android)：</td>
                <td>
                    <input type="text" name="SOAPAndroid" />
                </td>
            </tr>
            <tr>
                <td colspan="3">
                    <span style="color:red;">
                        【特殊通讯值】填写时，不用单引号【'】以及不用中括号【[]】
                        <br />
                        例如(不符合格式):<br />
                        {'name':'value','key':1} 、<br />
                        [{"name":"value","key":1} ]
                    </span>

                </td>
            </tr>
        </table>
    </form>
</div>

