                    }
                    else {
                        oo.Types = 0 ^ 0 ^ 4 ^ 8 ^ 0 ^ 0 ^ 0;
                    }
                }
                else
                    oo.Types = 0 ^ 0 ^ 0 ^ 8 ^ 0 ^ 0 ^ 0;
                oo.UserType = $(this).find("td:eq(4)>label>select").val();
                if (oo.Value > 0) {
                    arrdata.push(oo);
