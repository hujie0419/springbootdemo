@{
    ViewBag.Title = "Row";
    Layout = null;
}
@model Tuhu.Provisioning.DataAccess.Entity.LuckyWheelDeatil

@if (Model != null)
{
<tr data-json="@(Newtonsoft.Json.JsonConvert.SerializeObject(Model))" ondblclick="wheelDeatil.edit(this)">
    <td>
        @Model.ID
    </td>
    <td>
       @(Model.UserRank == 0?"所有用户":Model.UserRank == 1?"LV1":Model.UserRank == 2?"LV2":Model.UserRank == 3?"LV3":"LV4")
    </td>
    <td>
        @(Model.Type==4?"优惠券":"")
    </td>
    <td>
        @Model.CouponRuleID
    </td>
    <td>@Model.MaxCoupon</td>
    <td><img src="@Model.BGImage" width="40" height="40" /></td>
    <td><img src="@Model.ContentImage" width="40" height="40" /></td>
    @*<td><img src="@Model.ChangeImage" width="40" height="40" /></td>*@
    <td>@Model.GetDescription</td>
    <td>@Model.GoDescription</td>
    @*<td>@Model.APPUrl</td>*@
    @*<td>@Model.WapUrl</td>*@
    @*<td>@Model.WwwUrl</td>*@
    @*<td>@Model.HandlerIOS</td>*@
    @*<td>@Model.SOAPIOS</td>*@
    @*<td>@Model.HandlerAndroid</td>*@
    @*<td>@Model.SOAPAndroid</td>*@
    <td>@Model.OrderBy</td>
</tr>
}
