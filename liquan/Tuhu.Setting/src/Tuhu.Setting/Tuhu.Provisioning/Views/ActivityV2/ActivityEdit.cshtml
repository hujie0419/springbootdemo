                    {
                        <option value="0" selected="selected">无</option>
                    }
                    else
                    {
                        <option value="0">无</option>
                    }
                    @if (Model.ActivityType == 1)
                    {
                        <option value="1" selected="selected">轮毂列表</option>
                    }
                    else
                    {
                        <option value="1">轮毂列表</option>
                    }
                    @if (Model.ActivityType == 3)
                    {
                        <option value="3" selected="selected">轮胎列表</option>
                    }
                    else
                    {
                        <option value="3">轮胎列表</option>
                    }
                </select>
            </span>
           
            <span>分享参数:</span>
            <span>
                <input type="text" id="DataParames" name="DataParames" value="@Model.DataParames" style="width:180px;" />
            </span>
            <span>
                菜单类型:
            </span>
            <span>

                <select id="rbMenuType" name="rbMenuType">
                    @if (Model.MenuType == -1)
