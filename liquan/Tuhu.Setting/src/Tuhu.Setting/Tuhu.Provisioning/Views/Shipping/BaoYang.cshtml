        font-weight: normal;
    }

    input {
        width: 50px;
    }

    .bottom {
        clear: both;
        font-size: 30px;
        margin-top: 40px;
        text-align: center;
    }

    .bottom > a {
        color: black;
    }

    .bottom > a:hover {
        text-decoration: none;
        font-weight: bold;
    }

    .bottom > a:first-of-type {
        background-color: aquamarine;
        padding: 10px 20px;
        margin-right: 20px;
    }

    #tabRule input {
        width: auto;
    }

    button {
        display: inline-block;
        padding: 6px 12px;
        margin-bottom: 0;
        font-size: 14px;
        font-weight: 400;
        line-height: 1.42857143;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -ms-touch-action: manipulation;
        touch-action: manipulation;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        background-image: none;
        border: 1px solid transparent;
        border-radius: 4px;
    }

    .btn-primary {
        font-weight: bold;
        color: #fff;
        background-color: #5cb85c;
        border-color: #4cae4c;
        width: 80px;
    }

    .btn {
        display: inline-block;
        padding: 6px 12px;
        margin-bottom: 0;
        font-size: 14px;
        font-weight: 400;
        line-height: 1.42857143;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -ms-touch-action: manipulation;
        touch-action: manipulation;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        background-image: none;
        border: 1px solid transparent;
        border-radius: 4px;
    }

    .btn-danger {
        font-weight: bold;
        color: #fff;
        background-color: #d9534f;
        border-color: #d43f3a;
        width: 80px;
    }


    .correctPos {
        margin-left: 10px;
    }

    #DefaultRule input, #NewUserDefaultRule input { width: auto; }
</style>


<div style="clear:both;"></div>

<div style="float:left;" class="title">
    <h2>
        <span>@(ViewBag.Where == "home" ? "配送到家车品/保养订单 运费规则" : "配送到店车品/保养订单 运费规则")</span>
    </h2>
    <h3>
        <span>&nbsp;&nbsp;商品类别：</span><label>车品/保养</label>
    </h3>
    <h3>
        <span>&nbsp;&nbsp;配送方式：</span><label>@(ViewBag.Where == "home" ? "配送到家" : "配送到店")</label>
    </h3>
    <h3>
        <span style="color: red;">*</span><span>默认运费：</span>
        @{
            var defaultNoFree = Model.Where(m => (m.Types & 1) == 1 && (m.Types & 8) == 0);
            var defaultFree = Model.Where(m => (m.Types & 1) == 1 && (m.Types & 8) == 8);
            int userType = 1;

            if (defaultNoFree.Any())
            {
                userType = defaultNoFree.FirstOrDefault().UserType;
            }
            if (defaultFree.Any())
            {
                userType = defaultFree.FirstOrDefault().UserType;
            }
            <label>
                每单
                <input type="text" id="paid" name="default" data-pkid="@(defaultNoFree.Any() ? defaultNoFree.FirstOrDefault().PKID : 0)" value="@(defaultNoFree.Any() ? defaultNoFree.FirstOrDefault().Value : 0)" />元
                ，
            </label>
            <label>
                每单满
                <input type="text" id="free" name="default" data-pkid="@(defaultFree.Any() ? defaultFree.FirstOrDefault().PKID : 0)" value="@(defaultFree.Any() ? defaultFree.FirstOrDefault().Value : 0)" />元
                包邮
            </label>
            if (ViewBag.Where != "home")
            {
                <label>
                    ，包邮的订单金额
                    <select id="IsInstallFree">
                        <option value="1" @(Model.Any(m => (m.Types & 1) == 1 && (m.Types & 8) == 8 && (m.Types & 32) == 32) ? "selected" : null)>包含</option>
                        <option value="0" @(Model.Any(m => (m.Types & 1) == 1 && (m.Types & 8) == 8 && (m.Types & 32) == 0) ? "selected" : null)>不包含</option>
                    </select> 安装费
                </label>
            }
        }       
        <label style="font-size:12px;color:#696969;">（除指定地区外,其余地区的运费采用"默认运费"）</label>
    </h3>
</div>
