    </sectionGroup>
  </configSections>
  <quartz>
    <add key="quartz.scheduler.instanceName" value="C调度服务" />

    <!--configure thread pool info-->
    <add key="quartz.threadPool.type" value="Quartz.Simpl.SimpleThreadPool, Quartz" />
    <add key="quartz.threadPool.threadCount" value="20" />
    <add key="quartz.threadPool.threadPriority" value="Normal" />

    <add key="quartz.plugin.xml.type" value="Quartz.Plugin.Xml.XMLSchedulingDataProcessorPlugin, Quartz" />

    <add key="quartz.plugin.xml.fileNames" value="~/quartz_jobs.config" />
    <add key="quartz.plugin.xml.scanInterval" value="30" />
  </quartz>
  <nosql provider="Tuhu.Nosql.Redis.RedisProvider, Tuhu.Nosql.Redis" sectionName="redisClient" prefix="CJob" file="Cache.config">
    <expiration>
    </expiration>
  </nosql>
  <redisClient abortOnConnectFail="false" allowAdmin="true" ssl="false" connectTimeout="500" database="1" proxy="Twemproxy">
    <hosts>
      <add host="172.16.20.233" cachePort="6479" />
    </hosts>
  </redisClient>
  <zookeeper sessionTimeOut="30000" server="172.16.20.1" masterSlavePath="/MasterSlave/Service" />
  <rabbitmq hostName="172.16.20.1" userName="test" password="itsme999" virtualHost="/dev" />
  <webSite>
    <add host="api.tuhu.cn" ip="CL-API-01,CL-API-02,CL-API-03,CL-API-04,CL-API-05,CL-API-06,CL-API-07" timeout="3000" />
    <add host="by.tuhu.cn" ip="CL-BY-01,CL-BY-02,CL-BY-03,CL-BY-04,CL-BY-05,CL-BY-06,CL-BY-07" timeout="3000" url="/baoyang/Index.html?pid=VE-XJWSJTYBK&amp;n=2009&amp;pl=2.0L" />
    <add host="item.tuhu.cn" ip="" timeout="3000" url="/List/Tires/1/CP_Tire_AspectRatio=60&amp;CP_Tire_Rim=18&amp;CP_Tire_Width=235&amp;d_Vech=VE-QJLLANDJIG.html" />
    <add host="www.tuhu.cn" ip="CL-WWW-01,CL-WWW-02,CL-WWW-03,CL-WWW-04,CL-WWW-05,CL-WWW-06,CL-WWW-07" timeout="3000" />
