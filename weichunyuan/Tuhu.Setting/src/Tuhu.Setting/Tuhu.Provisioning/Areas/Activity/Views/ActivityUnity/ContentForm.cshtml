    function columnNumberChang() {
        var activeIndex = $("#tabHeader .active").find("a").attr("href").substring(1, 3);
        var form = $('#form' + activeIndex);

        var value = parseInt($("#ColumnNumber", form).val());
        var templateType = $("input[name='TemplateType']:checked", form).val();
        if (value == 2) {
            $(".columnNumber", form).show();
            $(".columnNumberTemp", form).show();
            $(".productpoolDisplayWay").hide();
            if ($("#resultActive", form).val() == 1 && templateType != 1) {
                $("#IsProgressBar").removeAttr("disabled");
            }
        }
