@model IEnumerable<Tuhu.Service.Product.Models.ProductConfig.TireInstallmentConfigLog>
@{
    ViewBag.Title = "ShowTireInstallmentLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table style="width:100%">
    <tr>
        <th>PID</th>
        <th>操作人</th>
        <th>内容</th>
        <th>操作时间</th>
    </tr>
    @if (Model != null && Model.Any())
    {
        foreach (var log in Model)
        {
            <tr>
                <td>
                    @log.PID
                </td>
                <td>@log.User</td>
                <td>@log.Content</td>
                <td>@log.CreateDateTime</td>
            </tr>
        }
    }
</table>