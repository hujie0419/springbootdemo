@{
    ViewBag.Title = "活动信息";
    Layout = null;
}
@model Tuhu.Provisioning.DataAccess.Entity.ActivityBuild


<div style="padding:5px 0px 5px 5px">
    <form id="form1" class="easyui-form">
        <div style="margin-bottom:5px;">
            <span>活动名称:</span>
            <span>
                <input type="hidden" id="id" name="id" value="@(Model.id)" />
                <input type="text" id="Title" class="easyui-textbox" name="Title" value="@(Model.Title !=""?@Model.Title:null)" style="width:160px;" />
            </span>
            <span>负责人:</span>
            <span>
                <input type="text" id="PersonWheel" name="PersonWheel" value="@Model.PersonWheel" />
            </span>
            <span>开始时间:</span>
            <span>
                <input type="text" id="StartDT" class="Wdate" onclick="WdatePicker({ dateFmt :'yyyy-MM-dd HH:mm:ss'})" name="StartDT" value="@(Model.StartDT != null?Convert.ToDateTime(Model.StartDT.ToString()).ToString("yyyy-MM-dd HH:mm:ss"):"")" />
            </span>
            <span>结束时间:</span>
            <span>
                <input type="text" id="EndDate" class="Wdate" onclick="WdatePicker({ dateFmt :'yyyy-MM-dd HH:mm:ss'})" name="EndDate" value="@(Model.EndDate != null?Convert.ToDateTime(Model.EndDate.ToString()).ToString("yyyy-MM-dd HH:mm:ss"):"")" />
            </span>
            <span>活动类型：</span>
            <span>
                <select id="ActivityConfigType" name="ActivityConfigType">
                    <option  @(Model.ActivityConfigType == 0?"selected=\"selected\"":"" ) value="0">综合</option>
                    <option @(Model.ActivityConfigType == 1?"selected=\"selected\"":"" )  value="1">车品</option>
                    <option @(Model.ActivityConfigType == 2?"selected=\"selected\"":"" )  value="2">保养</option>
                    <option @(Model.ActivityConfigType == 3?"selected=\"selected\"":"" )  value="3">轮胎</option>
                    <option @(Model.ActivityConfigType == 4?"selected=\"selected\"":"" )  value="4">美容改装</option>
                    <option @(Model.ActivityConfigType == 5?"selected=\"selected\"":"" )  value="5">外部投放活动</option>
                </select>
            </span>
            <span>
                是否显示倒计时:
            </span>
            <span>
                <select id="rbDateTime" name="rbDateTime">
                    @if (Model.IsShowDate == 1)
                    {
                        <option value="1" selected="selected">显示倒计时</option>
                        <option value="0">不显示倒计时</option>
                    }
                    else
                    {
                        <option value="1">显示倒计时</option>
                        <option value="0" selected="selected">不显示倒计时</option>
                    }
                </select>

            </span>
            &nbsp;&nbsp;&nbsp;
            <span>是否开启轮胎适配:</span>
            <span>
                <select id="rbIsTireSize" name="rbIsTireSize">
                    <option value="">请选择</option>
                    <option value="1" @(Model.IsTireSize == 1 ? "selected=\"selected\"" : "")>全部</option>
                    <option value="2" @(Model.IsTireSize == 2 ? "selected=\"selected\"" : "")>轮胎</option>
                    <option value="3" @(Model.IsTireSize == 3 ? "selected=\"selected\"" : "")>轮毂</option>
                </select>
            </span>
            <br />
            <span>背景图片:</span>
            <span>
                <img id="bgImage" style="width:20px;height:20px;" src="@(Model.SBgImageUrl)" />
                <input type="hidden" id="hidBgImageUrl" value="@(Model.BgImageUrl)" />
                <input type="hidden" id="hidSBgImageUrl" value="@(Model.SBgImageUrl)" />
            </span>
            <span>
                <input id="bgUpFile" width="60" type="file" name="bgUpFile" onchange="UploadBgImg(this, 'bgImage', 'hidBgImageUrl', 'hidSBgImageUrl')" />
            </span>
            <span>
                背景颜色:
            </span>
            <span>
                <input type="text" id="BgColor" name="BgColor" value="@(Model.BgColor != ""?Model.BgColor:null)" style="width:160px" />
            </span>
            
            <span>轮胎品牌:</span>
            <span>
                @*<select id="TireBrand" name="TireBrand">
                        <option value="">不选择</option>
                    </select>*@
